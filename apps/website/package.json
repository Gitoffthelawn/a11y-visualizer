{
  "name": "@a11y-visualizer/website",
  "version": "6.0.2",
  "type": "module",
  "scripts": {
    "predev": "node ./scripts/sync-docs.mjs",
    "dev": "astro dev",
    "prebuild": "node ./scripts/sync-docs.mjs",
    "build": "astro build",
    "preview": "astro preview",
    "test": "vitest --run --config vitest.config.ts --pool=forks",
    "test:jsdom": "vitest --run --config vitest.config.ts --pool=forks",
    "lint": "prettier . --check && eslint . --ext ts,tsx,astro --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "prettier . --write && eslint . --ext ts,tsx,astro --fix",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@astrojs/react": "^4.3.0",
    "@astrojs/tailwind": "^6.0.2",
    "@eslint/js": "^9.28.0",
