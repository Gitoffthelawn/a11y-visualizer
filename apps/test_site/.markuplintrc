{
  "specs": {
    "\\.[jt]sx?$": "@markuplint/react-spec"
  },
  "parser": {
    "\\.[jt]sx?$": "@markuplint/jsx-parser"
  },
  "nodeRules": [
    {
      "selector": ".intentional-violation",
      "rules": {
        "attr-duplication": false,
        "attr-value-quotes": false,
        "case-sensitive-attr-name": false,
        "case-sensitive-tag-name": false,
        "character-reference": false,
        "class-naming": false,
        "deprecated-attr": false,
        "deprecated-element": false,
        "disallowed-element": [],
        "doctype": "always",
        "end-tag": false,
        "heading-levels": false,
        "id-duplication": false,
        "ineffective-attr": false,
        "invalid-attr": false,
        "label-has-control": false,
        "landmark-roles": false,
        "neighbor-popovers": false,
        "no-ambiguous-navigable-target-names": false,
        "no-boolean-attr-value": false,
        "no-consecutive-br": false,
        "no-default-value": false,
        "no-duplicate-dt": false,
        "no-empty-palpable-content": false,
        "no-hard-code-id": false,
        "no-orphaned-end-tag": false,
        "no-refer-to-non-existent-id": false,
        "no-use-event-handler-attr": false,
        "permitted-contents": false,
        "placeholder-label-option": false,
        "require-accessible-name": false,
        "require-datetime": false,
        "required-attr": [],
        "required-element": [],
        "required-h1": false,
        "table-row-column-alignment": false,
        "use-list": false,
        "wai-aria": false
      }
    }
  ],
  "rules": {
    "attr-duplication": true,
    "attr-value-quotes": false,
    "case-sensitive-attr-name": false,
    "case-sensitive-tag-name": false,
    "character-reference": true,
    "class-naming": false,
    "deprecated-attr": true,
    "deprecated-element": true,
    "disallowed-element": [],
    "doctype": "always",
    "end-tag": false,
    "heading-levels": true,
    "id-duplication": true,
    "ineffective-attr": false,
    "invalid-attr": true,
    "label-has-control": false,
    "landmark-roles": false,
    "neighbor-popovers": true,
    "no-ambiguous-navigable-target-names": false,
    "no-boolean-attr-value": false,
    "no-consecutive-br": false,
    "no-default-value": false,
    "no-duplicate-dt": true,
    "no-empty-palpable-content": false,
    "no-hard-code-id": false,
    "no-orphaned-end-tag": true,
    "no-refer-to-non-existent-id": true,
    "no-use-event-handler-attr": false,
    "permitted-contents": [],
    "placeholder-label-option": true,
    "require-accessible-name": true,
    "require-datetime": true,
    "required-attr": [],
    "required-element": [],
    "required-h1": true,
    "table-row-column-alignment": false,
    "use-list": false,
    "wai-aria": true
  },
  "overrides": {
    "index.html": {
      "rules": {
        "required-h1": false
      }
    },
    "iframe.html": {
      "rules": {
        "required-h1": false
      }
    },
    "components/**/*.tsx": {
      "rules": {
        "heading-levels": false
      }
    }
  }
}
